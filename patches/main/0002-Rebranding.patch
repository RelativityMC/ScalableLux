From 0000000000000000000000000000000000000000 Mon Sep 17 00:00:00 2001
From: ishland <ishlandmc@yeah.net>
Date: Sun, 17 Mar 2024 10:11:48 +0800
Subject: [PATCH] Rebranding


diff --git a/build.gradle b/build.gradle
index c9cf5f5..ffe886e 100644
--- a/build.gradle
+++ b/build.gradle
@@ -17,6 +17,7 @@ def getGitCommit = { ->
     exec {
         commandLine 'git', 'rev-parse', '--short', 'HEAD'
         standardOutput = stdout
+        workingDir = ".."
     }
     return stdout.toString().trim()
 }
@@ -55,7 +56,7 @@ tasks.withType(JavaCompile) {
 }
 
 loom {
-    accessWidenerPath = file("src/main/resources/starlight.accesswidener")
+    accessWidenerPath = file("src/main/resources/scalablelux.accesswidener")
 }
 
 // Loom will automatically attach sourcesJar to a RemapSourcesJar task and to the "build" task
diff --git a/gradle.properties b/gradle.properties
index 8f69d83..85f5b31 100644
--- a/gradle.properties
+++ b/gradle.properties
@@ -9,4 +9,4 @@ loader_version=0.15.1
 # Mod Properties
 mod_version=1.1.3
 maven_group=ca.spottedleaf.starlight
-archives_base_name=starlight
+archives_base_name=ScalableLux
diff --git a/settings.gradle b/settings.gradle
index b02216b..51c65a1 100644
--- a/settings.gradle
+++ b/settings.gradle
@@ -8,3 +8,5 @@ pluginManagement {
         gradlePluginPortal()
     }
 }
+
+rootProject.name = "ScalableLux"
diff --git a/src/main/resources/assets/starlight/icon.png b/src/main/resources/assets/starlight/icon.png
deleted file mode 100644
